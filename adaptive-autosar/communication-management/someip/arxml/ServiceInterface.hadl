    }

    struct EvDrvRdySta_Resp {
        status of uint8_t
    }
    
    struct PrkBrkSta {
        status of uint8_t
    }
*/
	// definition of software components in the application design
    component Publisher_Component {
        provide PPort for VehicleControlService
    }

    component Subscriber_Component {
        require RPort for VehicleControlService
    }
}
