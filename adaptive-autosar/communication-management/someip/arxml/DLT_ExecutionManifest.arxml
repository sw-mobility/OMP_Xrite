<?xml version="1.0" encoding="UTF-8"?>
<AUTOSAR xmlns="http://autosar.org/schema/r4.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://autosar.org/schema/r4.0 AUTOSAR_00050.xsd">
  <AR-PACKAGES>
    <AR-PACKAGE>
      <SHORT-NAME>RB</SHORT-NAME>
      <AR-PACKAGES>
        <AR-PACKAGE>
          <SHORT-NAME>VRTE</SHORT-NAME>
          <AR-PACKAGES>
            <AR-PACKAGE>
              <SHORT-NAME>Executables</SHORT-NAME>
              <ELEMENTS>
                <EXECUTABLE>
                  <SHORT-NAME>DLT</SHORT-NAME>
                  <ADMIN-DATA>
                    <SDGS>
                      <SDG GID="VRTE">
                        <SD GID="path">/opt/vrte/dev-aap-dlt/bin/rb-dltd</SD>
                      </SDG>
                    </SDGS>
                  </ADMIN-DATA>
                  <REPORTING-BEHAVIOR>REPORTS-EXECUTION-STATE</REPORTING-BEHAVIOR>
                </EXECUTABLE>
              </ELEMENTS>
            </AR-PACKAGE>
            <AR-PACKAGE>
              <SHORT-NAME>Processes</SHORT-NAME>
              <ELEMENTS>
                <PROCESS>
                  <SHORT-NAME>DLT_A</SHORT-NAME>
                  <ADMIN-DATA>
                    <SDGS>
                      <SDG GID="VRTE">
                        <SD GID="uid">0</SD>
                        <SD GID="gid">0</SD>
                      </SDG>
                    </SDGS>
                  </ADMIN-DATA>
                  <EXECUTABLE-REF DEST="EXECUTABLE">/RB/VRTE/Executables/DLT</EXECUTABLE-REF>
                  <STATE-DEPENDENT-STARTUP-CONFIGS>
                    <STATE-DEPENDENT-STARTUP-CONFIG>
                      <FUNCTION-GROUP-STATE-IREFS>
                        <FUNCTION-GROUP-STATE-IREF>
                          <CONTEXT-MODE-DECLARATION-GROUP-PROTOTYPE-REF DEST="MODE-DECLARATION-GROUP-PROTOTYPE">/FunctionGroupSets/FunctionGroupSet_0/MachineFG</CONTEXT-MODE-DECLARATION-GROUP-PROTOTYPE-REF>
                          <TARGET-MODE-DECLARATION-REF DEST="MODE-DECLARATION">/ModeDeclarationGroups/MachineFG/Restart</TARGET-MODE-DECLARATION-REF>
                        </FUNCTION-GROUP-STATE-IREF>
                        <FUNCTION-GROUP-STATE-IREF>
                          <CONTEXT-MODE-DECLARATION-GROUP-PROTOTYPE-REF DEST="MODE-DECLARATION-GROUP-PROTOTYPE">/FunctionGroupSets/FunctionGroupSet_0/MachineFG</CONTEXT-MODE-DECLARATION-GROUP-PROTOTYPE-REF>
                          <TARGET-MODE-DECLARATION-REF DEST="MODE-DECLARATION">/ModeDeclarationGroups/MachineFG/Shutdown</TARGET-MODE-DECLARATION-REF>
                        </FUNCTION-GROUP-STATE-IREF>
                        <FUNCTION-GROUP-STATE-IREF>
                          <CONTEXT-MODE-DECLARATION-GROUP-PROTOTYPE-REF DEST="MODE-DECLARATION-GROUP-PROTOTYPE">/FunctionGroupSets/FunctionGroupSet_0/MachineFG</CONTEXT-MODE-DECLARATION-GROUP-PROTOTYPE-REF>
                          <TARGET-MODE-DECLARATION-REF DEST="MODE-DECLARATION">/ModeDeclarationGroups/MachineFG/Startup</TARGET-MODE-DECLARATION-REF>
                        </FUNCTION-GROUP-STATE-IREF>
                      </FUNCTION-GROUP-STATE-IREFS>
                      <STARTUP-CONFIG-REF DEST="STARTUP-CONFIG">/RB/VRTE/StartupConfigs/DLT_StartupConfig</STARTUP-CONFIG-REF>
                    </STATE-DEPENDENT-STARTUP-CONFIG>
                  </STATE-DEPENDENT-STARTUP-CONFIGS>
                </PROCESS>
                <PROCESS>
                  <SHORT-NAME>DLT_B</SHORT-NAME>
                  <ADMIN-DATA>
                    <SDGS>
                      <SDG GID="VRTE">
                        <SD GID="uid">0</SD>
                        <SD GID="gid">0</SD>
                      </SDG>
                    </SDGS>
                  </ADMIN-DATA>
                  <EXECUTABLE-REF DEST="EXECUTABLE">/RB/VRTE/Executables/DLT</EXECUTABLE-REF>
                  <STATE-DEPENDENT-STARTUP-CONFIGS>
                    <STATE-DEPENDENT-STARTUP-CONFIG>
                      <FUNCTION-GROUP-STATE-IREFS>
                        <FUNCTION-GROUP-STATE-IREF>
                          <CONTEXT-MODE-DECLARATION-GROUP-PROTOTYPE-REF DEST="MODE-DECLARATION-GROUP-PROTOTYPE">/FunctionGroupSets/FunctionGroupSet_1/MachineFG</CONTEXT-MODE-DECLARATION-GROUP-PROTOTYPE-REF>
                          <TARGET-MODE-DECLARATION-REF DEST="MODE-DECLARATION">/ModeDeclarationGroups/MachineFG/Restart</TARGET-MODE-DECLARATION-REF>
                        </FUNCTION-GROUP-STATE-IREF>
                        <FUNCTION-GROUP-STATE-IREF>
                          <CONTEXT-MODE-DECLARATION-GROUP-PROTOTYPE-REF DEST="MODE-DECLARATION-GROUP-PROTOTYPE">/FunctionGroupSets/FunctionGroupSet_1/MachineFG</CONTEXT-MODE-DECLARATION-GROUP-PROTOTYPE-REF>
                          <TARGET-MODE-DECLARATION-REF DEST="MODE-DECLARATION">/ModeDeclarationGroups/MachineFG/Shutdown</TARGET-MODE-DECLARATION-REF>
                        </FUNCTION-GROUP-STATE-IREF>
                        <FUNCTION-GROUP-STATE-IREF>
                          <CONTEXT-MODE-DECLARATION-GROUP-PROTOTYPE-REF DEST="MODE-DECLARATION-GROUP-PROTOTYPE">/FunctionGroupSets/FunctionGroupSet_1/MachineFG</CONTEXT-MODE-DECLARATION-GROUP-PROTOTYPE-REF>
                          <TARGET-MODE-DECLARATION-REF DEST="MODE-DECLARATION">/ModeDeclarationGroups/MachineFG/Startup</TARGET-MODE-DECLARATION-REF>
                        </FUNCTION-GROUP-STATE-IREF>
                        <FUNCTION-GROUP-STATE-IREF>
                          <CONTEXT-MODE-DECLARATION-GROUP-PROTOTYPE-REF DEST="MODE-DECLARATION-GROUP-PROTOTYPE">/FunctionGroupSets/FunctionGroupSet_1/MachineFG</CONTEXT-MODE-DECLARATION-GROUP-PROTOTYPE-REF>
                          <TARGET-MODE-DECLARATION-REF DEST="MODE-DECLARATION">/ModeDeclarationGroups/MachineFG/Verify</TARGET-MODE-DECLARATION-REF>
                        </FUNCTION-GROUP-STATE-IREF>
                      </FUNCTION-GROUP-STATE-IREFS>
                      <STARTUP-CONFIG-REF DEST="STARTUP-CONFIG">/RB/VRTE/StartupConfigs/DLT_B_StartupConfig</STARTUP-CONFIG-REF>
                    </STATE-DEPENDENT-STARTUP-CONFIG>
                  </STATE-DEPENDENT-STARTUP-CONFIGS>
                </PROCESS>
              </ELEMENTS>
            </AR-PACKAGE>
            <AR-PACKAGE>
              <SHORT-NAME>StartupConfigs</SHORT-NAME>
              <ELEMENTS>
                <STARTUP-CONFIG>
                  <SHORT-NAME>DLT_StartupConfig</SHORT-NAME>
                  <ENVIRONMENT-VARIABLES>
                    <TAG-WITH-OPTIONAL-VALUE>
                      <KEY>LD_LIBRARY_PATH</KEY>
                      <VALUE>/lib:/usr/lib:/opt/vrte/lib</VALUE>
                    </TAG-WITH-OPTIONAL-VALUE>
                    <TAG-WITH-OPTIONAL-VALUE>
                      <KEY>ECUCFG_ENV_VAR_ROOTFOLDER</KEY>
                      <VALUE>/opt/vrte/dev-aap-dlt/etc/ecu-cfg</VALUE>
                    </TAG-WITH-OPTIONAL-VALUE>
                  </ENVIRONMENT-VARIABLES>
                  <PROCESS-ARGUMENTS>
                    <PROCESS-ARGUMENT>
                      <ARGUMENT>-c</ARGUMENT>
                    </PROCESS-ARGUMENT>
                    <PROCESS-ARGUMENT>
                      <ARGUMENT>/opt/vrte/usr/etc/dlt_a.conf</ARGUMENT>
                    </PROCESS-ARGUMENT>
                    <PROCESS-ARGUMENT>
                      <ARGUMENT>-t</ARGUMENT>
                    </PROCESS-ARGUMENT>
                    <PROCESS-ARGUMENT>
                      <ARGUMENT>/opt/vrte/tmp/dlt</ARGUMENT>
                    </PROCESS-ARGUMENT>
                  </PROCESS-ARGUMENTS>
                  <TIMEOUT>
                    <ENTER-TIMEOUT-VALUE>100.0</ENTER-TIMEOUT-VALUE>
                    <EXIT-TIMEOUT-VALUE>100.0</EXIT-TIMEOUT-VALUE>
                  </TIMEOUT>
                </STARTUP-CONFIG>
                <STARTUP-CONFIG>
                  <SHORT-NAME>DLT_B_StartupConfig</SHORT-NAME>
                  <ENVIRONMENT-VARIABLES>
                    <TAG-WITH-OPTIONAL-VALUE>
                      <KEY>LD_LIBRARY_PATH</KEY>
                      <VALUE>/lib:/usr/lib:/opt/vrte/lib</VALUE>
                    </TAG-WITH-OPTIONAL-VALUE>
                    <TAG-WITH-OPTIONAL-VALUE>
                      <KEY>ECUCFG_ENV_VAR_ROOTFOLDER</KEY>
                      <VALUE>/opt/vrte/dev-aap-dlt/etc/ecu-cfg</VALUE>
                    </TAG-WITH-OPTIONAL-VALUE>
                  </ENVIRONMENT-VARIABLES>
                  <PROCESS-ARGUMENTS>
                    <PROCESS-ARGUMENT>
                      <ARGUMENT>-c</ARGUMENT>
                    </PROCESS-ARGUMENT>
                    <PROCESS-ARGUMENT>
                      <ARGUMENT>/opt/vrte/usr/etc/dlt_b.conf</ARGUMENT>
                    </PROCESS-ARGUMENT>
                    <PROCESS-ARGUMENT>
                      <ARGUMENT>-t</ARGUMENT>
                    </PROCESS-ARGUMENT>
                    <PROCESS-ARGUMENT>
                      <ARGUMENT>/opt/vrte/tmp/dlt</ARGUMENT>
                    </PROCESS-ARGUMENT>
                  </PROCESS-ARGUMENTS>
                  <TIMEOUT>
                    <ENTER-TIMEOUT-VALUE>100.0</ENTER-TIMEOUT-VALUE>
                    <EXIT-TIMEOUT-VALUE>100.0</EXIT-TIMEOUT-VALUE>
                  </TIMEOUT>
                </STARTUP-CONFIG>
              </ELEMENTS>
            </AR-PACKAGE>
            <AR-PACKAGE>
              <SHORT-NAME>ProcessToMachineMappings</SHORT-NAME>
              <ELEMENTS>
                <PROCESS-TO-MACHINE-MAPPING-SET>
                  <SHORT-NAME>DLT_ExecutionManifest_ProcessToMachineMappingSet</SHORT-NAME>
                  <PROCESS-TO-MACHINE-MAPPINGS>
                    <PROCESS-TO-MACHINE-MAPPING>
                      <SHORT-NAME>DLT_A_Machine_A_Mapping</SHORT-NAME>
                      <MACHINE-REF DEST="MACHINE">/Machines/Machine_A</MACHINE-REF>
                      <PROCESS-REF DEST="PROCESS">/RB/VRTE/Processes/DLT_A</PROCESS-REF>
                    </PROCESS-TO-MACHINE-MAPPING>
                    <PROCESS-TO-MACHINE-MAPPING>
                      <SHORT-NAME>DLT_B_Machine_B_Mapping</SHORT-NAME>
                      <MACHINE-REF DEST="MACHINE">/Machines/Machine_B</MACHINE-REF>
                      <PROCESS-REF DEST="PROCESS">/RB/VRTE/Processes/DLT_B</PROCESS-REF>
                    </PROCESS-TO-MACHINE-MAPPING>
                  </PROCESS-TO-MACHINE-MAPPINGS>
                </PROCESS-TO-MACHINE-MAPPING-SET>
              </ELEMENTS>
            </AR-PACKAGE>
            <AR-PACKAGE>
              <SHORT-NAME>DltLogSinks</SHORT-NAME>
              <ELEMENTS>
                <DLT-LOG-SINK>
                  <SHORT-NAME>EXM_Remote</SHORT-NAME>
                  <CATEGORY>DLT_LOGSINK_REMOTE</CATEGORY>
                  <DEFAULT-LOG-THRESHOLD>INFO</DEFAULT-LOG-THRESHOLD>
                </DLT-LOG-SINK>
                <DLT-LOG-SINK>
                  <SHORT-NAME>EXM_Console</SHORT-NAME>
                  <CATEGORY>DLT_LOGSINK_CONSOLE</CATEGORY>
                  <DEFAULT-LOG-THRESHOLD>INFO</DEFAULT-LOG-THRESHOLD>
                </DLT-LOG-SINK>
              </ELEMENTS>
            </AR-PACKAGE>
            <AR-PACKAGE>
              <SHORT-NAME>DltLogSinkToPortPrototypeMappings</SHORT-NAME>
            </AR-PACKAGE>
            <AR-PACKAGE>
              <SHORT-NAME>DltContexts</SHORT-NAME>
            </AR-PACKAGE>
            <AR-PACKAGE>
              <SHORT-NAME>DltContextToPortPrototypeMappings</SHORT-NAME>
            </AR-PACKAGE>
            <AR-PACKAGE>
              <SHORT-NAME>DltApplicationToProcessMappings</SHORT-NAME>
            </AR-PACKAGE>
          </AR-PACKAGES>
        </AR-PACKAGE>
      </AR-PACKAGES>
    </AR-PACKAGE>
  </AR-PACKAGES>
</AUTOSAR>
