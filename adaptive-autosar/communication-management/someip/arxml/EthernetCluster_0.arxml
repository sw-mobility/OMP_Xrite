<?xml version="1.0" encoding="UTF-8"?>
<AUTOSAR xmlns="http://autosar.org/schema/r4.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://autosar.org/schema/r4.0 AUTOSAR_00050.xsd">
  <AR-PACKAGES>
    <AR-PACKAGE>
      <SHORT-NAME>CommunicationClusters</SHORT-NAME>
      <ELEMENTS>
        <ETHERNET-CLUSTER>
          <SHORT-NAME>EthernetCluster_0</SHORT-NAME>
          <ETHERNET-CLUSTER-VARIANTS>
            <ETHERNET-CLUSTER-CONDITIONAL>
              <PHYSICAL-CHANNELS>
                <ETHERNET-PHYSICAL-CHANNEL>
                  <SHORT-NAME>EthernetPhysicalChannel_0</SHORT-NAME>
                  <NETWORK-ENDPOINTS>
                    <NETWORK-ENDPOINT>
                      <SHORT-NAME>NetworkEndpoint_A</SHORT-NAME>
                      <NETWORK-ENDPOINT-ADDRESSES>
                        <IPV-4-CONFIGURATION>
                          <IPV-4-ADDRESS>${PUBLISHER_IP}</IPV-4-ADDRESS>
                          <IPV-4-ADDRESS-SOURCE>FIXED</IPV-4-ADDRESS-SOURCE>
                          <NETWORK-MASK>255.255.0.0</NETWORK-MASK>
                        </IPV-4-CONFIGURATION>
                      </NETWORK-ENDPOINT-ADDRESSES>
                    </NETWORK-ENDPOINT>
                    <NETWORK-ENDPOINT>
                      <SHORT-NAME>Multicast_A</SHORT-NAME>
                      <NETWORK-ENDPOINT-ADDRESSES>
                        <IPV-4-CONFIGURATION>
                          <IP-ADDRESS-KEEP-BEHAVIOR>STORE-PERSISTENTLY</IP-ADDRESS-KEEP-BEHAVIOR>
                          <IPV-4-ADDRESS>224.244.224.245</IPV-4-ADDRESS>
                          <NETWORK-MASK>255.255.0.0</NETWORK-MASK>
                        </IPV-4-CONFIGURATION>
                      </NETWORK-ENDPOINT-ADDRESSES>
                    </NETWORK-ENDPOINT>
                    <NETWORK-ENDPOINT>
                      <SHORT-NAME>Multicast_B</SHORT-NAME>
                      <NETWORK-ENDPOINT-ADDRESSES>
                        <IPV-4-CONFIGURATION>
                          <IP-ADDRESS-KEEP-BEHAVIOR>STORE-PERSISTENTLY</IP-ADDRESS-KEEP-BEHAVIOR>
                          <IPV-4-ADDRESS>224.244.224.245</IPV-4-ADDRESS>
                          <NETWORK-MASK>255.255.0.0</NETWORK-MASK>
                        </IPV-4-CONFIGURATION>
                      </NETWORK-ENDPOINT-ADDRESSES>
                    </NETWORK-ENDPOINT>
                    <NETWORK-ENDPOINT>
                      <SHORT-NAME>NetworkEndpoint_B</SHORT-NAME>
                      <NETWORK-ENDPOINT-ADDRESSES>
                        <IPV-4-CONFIGURATION>
                          <IPV-4-ADDRESS>${SUBSCRIBER_IP}</IPV-4-ADDRESS>
                          <NETWORK-MASK>255.255.0.0</NETWORK-MASK>
                        </IPV-4-CONFIGURATION>
                      </NETWORK-ENDPOINT-ADDRESSES>
                    </NETWORK-ENDPOINT>
                  </NETWORK-ENDPOINTS>
                </ETHERNET-PHYSICAL-CHANNEL>
              </PHYSICAL-CHANNELS>
            </ETHERNET-CLUSTER-CONDITIONAL>
          </ETHERNET-CLUSTER-VARIANTS>
        </ETHERNET-CLUSTER>
      </ELEMENTS>
    </AR-PACKAGE>
  </AR-PACKAGES>
</AUTOSAR>
